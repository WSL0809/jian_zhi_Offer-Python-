   "source": [
    "#方法1：遍历添加\n",
    "def replaceSpace1(s:str) -> str:\n",
    "    res = []\n",
    "    for c in s:\n",
    "        if c == ' ':\n",
    "            res.append(\"%20\")\n",
    "        else:\n",
    "            res.append(c)\n",
    "    return \"\".join(res)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Ear%20Trumpet\n"
     ]
    }
   ],
   "source": [
    "s = \"Ear Trumpet\"\n",
    "print(replaceSpace1(s))"
   ]
  }
